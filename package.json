    "@voiceflow/eslint-config": "6.1.0",
    "@voiceflow/git-branch-check": "1.4.0",
    "@voiceflow/prettier-config": "1.2.1",
    "@voiceflow/tsconfig": "1.4.8",
    "babel-loader": "^8.2.5",
