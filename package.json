  "files": [
    "build/",
    "dist/",
    "iframe/dist/"
  ],
  "homepage": "https://github.com/voiceflow/react-chat#readme",
  "keywords": [
    "combinedReducer",
    "react",
    "react-redux",
    "reducer",
    "redux",
    "voiceflow"
  ],
  "license": "ISC",
  "main": "build/cjs/index.js",
  "prettier": "@voiceflow/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voiceflow/react-chat.git"
  },
  "scripts": {
    "build": "yarn clean && yarn build:package && yarn build:bundle && yarn iframe:build",
    "build:bundle": "NODE_ENV=production vite build",
    "build:cjs": "tsc --project ./tsconfig.build.cjs.json && tsc-alias -p tsconfig.build.cjs.json",
    "build:docs": "STORYBOOK_BASE_HREF=react-chat build-storybook -o docs",
    "build:esm": "tsc --project ./tsconfig.build.esm.json && tsc-alias -p tsconfig.build.esm.json",
    "build:package": "NODE_ENV=production yarn build:cjs && yarn build:esm",
    "clean": "rimraf build dist",
    "commit": "cz",
    "eslint-output": "eslint-output",
    "iframe:build": "NODE_ENV=production cd iframe && vite build",
    "iframe:start": "cd iframe && vite --config vite.config.ts",
    "lint": "eslint '**/*.{js,ts,tsx,mdx}'",
    "lint:output": "yarn run eslint-output --quiet \"**/*.{js,ts,tsx,mdx}\"",
    "lint:quiet": "yarn lint --quiet",
    "lint:report": "yarn lint:output",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "start": "NODE_ENV=development concurrently -k -c yellow,blue 'vite --config vite.config.ts' 'yarn iframe:start'",
    "storybook": "start-storybook -p 6006",
    "test": "yarn test:run",
    "test:dependencies": "depcheck",
    "test:unit": "NODE_ENV=test vitest run --coverage"
  },
  "types": "build/esm/index.d.ts"
}
